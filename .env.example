# Proxy Configuration
PROXY_HOST=p.webshare.io
PROXY_PORT=80
PROXY_ROTATE=your_username
PROXY_PASSWORD=your_password
PROXY_PROTOCOL=http
PROXY_VERIFY_SSL=false
PROXY_USE_HTTPS=false
PROXY_TIMEOUT=30.0
PROXY_MAX_RETRIES=3
PROXY_RETRY_DELAY=2.0
PROXY_VALIDATE_CERT=false
PROXY_ALLOW_INSECURE=false
PROXY_ROTATING=true
PROXY_BYPASS_SYSTEM=true

# Required Fields
DOMAIN=your_domain
PORT=your_port
PROTOCOL=http

# Output Directory
CSV_OUTPUT_DIR=output

# Security Settings (Optional)
SKIP_PROXY_SECURITY=false
ALLOW_DIRECT_CONNECTION=false

# Logging Settings
LOG_LEVEL=INFO
LOG_FORMAT="%(asctime)s - %(name)s - %(levelname)s - %(message)s"
LOG_DATE_FORMAT="%Y-%m-%d %H:%M:%S" 